{
  "mode": "All",
  "policyRule": {
    "if": {
      "allOf": [
        {
          "field": "type",
          "equals": "Microsoft.Sql/servers/databases"
        },
        {
          "field": "Microsoft.Sql/servers/databases/edition",
          "notEquals": "DataWarehouse"
        }
      ]
    },
    "then": {
      "effect": "deny"
    }
  }
}